def pop(discipline):
  print("i love "+discipline)

pop("PP2")
pop("CALCULUS2")
pop("SOCIOLOGY")



def pop(fname, lname):
  print(fname + " " + lname+" are my favorite disciplines")

pop("PP2", "CALCULUC2")




def my_function(fname, lname):
  print(fname + " " + lname)

my_function("Emil")




def my_function(name = "friend"):
  print("Hello", name)

my_function("Emil")
my_function("Tobias")
my_function()
my_function("Linus")




def my_function(country = "Norway"):
  print("I am from", country)

my_function("Sweden")
my_function("India")
my_function()
my_function("Brazil")
